{
  "name": "athom-api",
  "version": "2.1.178",
  "description": "Bundles all Homey API's",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "to-staging": "git checkout staging; git pull; git merge master --ff; git checkout master",
    "to-testing": "git checkout testing; git pull; git merge master --ff; git checkout master",
    "build": "grunt build && grunt build-docs && if ! git diff --no-ext-diff --exit-code --quiet; then git add -A && git commit -m 'Update APIs [ci skip]'; fi",
    "prepack": "if [ -d docs ]; then mv docs/index.html docs/index2.html && sed \"s/\\${NPM_VERSION}/${npm_package_version}/g\" docs/index2.html > docs/index.html && rm -f docs/index2.html; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/athombv/node-homey-api.git"
  },
  "author": "Jeroen Vollenbrock <jeroen@athom.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/athombv/node-athom-api/issues"
  },
  "homepage": "https://github.com/athombv/node-athom-api#readme",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es5-property-mutators": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify-derequire": "^0.9.4",
    "form-data": "^2.3.1",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^3.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-jsdoc": "^2.1.0",
    "jsdoc-rtd": "git+ssh://git@github.com/athombv/jsdoc-rtd.git",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "mustache": "^2.3.0",
    "node-fetch": "^1.7.1",
    "prompt": "^1.0.0",
    "socket.io-client": "1.4.8",
    "whatwg-fetch-timeout": "^2.0.2-timeout"
  },
  "config": {
    "buildDir": "./docs",
    "npmPublishTagStaging": "beta",
    "npmPublishTagTesting": "alpha",
    "npmPublishTagProduction": "latest",
    "s3BucketStaging": "athom-developer-api",
    "s3BucketProduction": "athom-developer-api",
    "s3BucketTesting": "athom-developer-api-testing",
    "nodeVersion": "8"
  }
}
